{
  "name": "angelomarafelli/doop-modules",
  "description": "Gerenciamento de Módulos para o Framework doop baseado em Laravel 5.5",
  "keywords": [
    "modules",
    "laravel",
    "doop",
    "module"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ângelo Marafelli",
      "email": "comercial@devremote.com.br",
      "homepage": "https://www.devremote.com.br",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.4.*",
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "~0.9",
    "orchestra/testbench": "^3.1|^3.2|^3.3|^3.4",
    "phpro/grumphp": "^0.9.1",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "DOOP\\Modules\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DOOP\\Modules\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
